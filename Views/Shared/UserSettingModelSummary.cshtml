@model UserSettingAddModel

<div class="content">
    <div class="row">
        <div class="col-md-8">
            <div class="card">
                <div class="card-header">
                    <h5 class="title">Edit Profile</h5>
                </div>
                <div class="card-body">
                    <form asp-action="EditSetting" asp-controller="Account" asp-antiforgery="true">
                        <div class="validation" asp-validation-summary="ModelOnly"></div>
                        <div>
                            <div class="form-group">
                                <label asp-for="VkToken">Введите VkToken</label>
                                <input type="text" asp-for="VkToken" />
                                <span asp-validation-for="VkToken" ></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="VkApiVersion">Введите VkApiVersion</label>
                                <input type="text" asp-for="VkApiVersion" />
                                <span asp-validation-for="VkApiVersion" ></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="TelegramToken">Введите TelegramToken</label>
                                <input type="text" asp-for="TelegramToken" />
                                <span asp-validation-for="TelegramToken" ></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="TelegramGroup">Введите TelegramGroup</label>
                                <input type="text" asp-for="TelegramGroup" />
                                <span asp-validation-for="TelegramGroup" ></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Deley">Введите задержку постинга</label>
                                <input type="text" asp-for="Deley" />
                                <span asp-validation-for="Deley" ></span>
                            </div>
                            <div class="form-group">
                                <input type="submit" value="Изменить" class="btn btn-outline-dark" />
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>