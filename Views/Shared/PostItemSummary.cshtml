@model PostSetting

<tr>
    <td>@Model.GroupUrl</td>
    <td>@Model.PostCount</td>
    <td>@Model.PostFilter</td>
    <td> 
        <a asp-action="EditPostSetting" asp-controller="Post" 
            asp-route-PostSettingId="@Model.PostSettingId" class="btn btn-primary" >Update</a> 
        <a asp-action="DeletePostSetting" asp-controller="Post"
            asp-route-PostSettingId="@Model.PostSettingId" class="btn btn-danger" >Delete</a> 
    </td>
    <td></td>
</tr> 