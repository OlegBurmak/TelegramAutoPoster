<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TAPoster</title>
    <link asp-href-include="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" asp-href-include="~/css/site.css" />
    <link rel="stylesheet" asp-href-include="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link asp-href-include="css/sb-admin-2.min.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    
</head>
<body>    

    @RenderBody()

    <environment include="Development">
        <script asp-src-include="~/vendor/jquery/jquery.min.js"></script>
        <script asp-src-include="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script asp-src-include="~/vendor/jquery-easing/jquery.easing.min.js"></script>

        <!-- Custom scripts for all pages-->
        <script asp-src-include="~/js/sb-admin-2.min.js"></script>

        <!-- Page level plugins -->
        <script asp-src-include="~/vendor/chart.js/Chart.min.js"></script>

        <!-- Page level custom scripts -->
        <script asp-src-include="~/js/demo/chart-area-demo.js"></script>
        <script asp-src-include="~/js/demo/chart-pie-demo.js"></script>
        
    </environment>

    <environment exclude="Development">
        
    </environment>
</body>
</html>
